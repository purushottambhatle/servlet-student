<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet-context>
		<param-name>school</param-name>
		<param-value>DSP</param-value>
	</servlet-context>
	<servlet>
		<servlet-name>sign-up</servlet-name>
		<servlet-class>com.ty.servlet_eb9_prc_stack.controller.StudentController</servlet-class>
		<init-param>
			<param-name>fee</param-name>
			<param-value>30000</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>sign-up</servlet-name>
		<url-pattern>/sign-up</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.ty.servlet_eb9_prc_stack.controller.LoginStudent</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

</web-app>
